  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "importHelpers": true,
    "target": "es5",
    "module": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "lib": ["dom", "es5", "es6"]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}