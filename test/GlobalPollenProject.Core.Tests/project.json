{
  "version": "0.1.0-*",
  "buildOptions": {
    "emitEntryPoint": false,
    "compilerName": "fsc",
    "compile": {
      "includeFiles": [
        "Specification.fs",
        "Grain.fs"
      ]
    }
  },
  "dependencies": {
    "xunit":"2.1.0",
    "dotnet-test-xunit":"1.0.0-*",
    "GlobalPollenProject.Core": {
      "target": "project"
    }
  },
  "tools": {
    "xunit.runner.console":"2.1.0",
    "dotnet-compile-fsc": {
      "version": "1.0.0-*",
      "imports": [
        "dnxcore50",
        "portable-net45+win8",
        "netstandard1.3"
      ]
    }
  },  
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.FSharp.Core.netcore":"1.0.0-alpha-160509",
        "Microsoft.NETCore.App": {
          "type":"platform",
          "version":"1.0.0-rc2-3002702"
        }
      },
      "imports": [
        "portable-net451+win81",
        "dnxcore50"
      ]
    }
  },
  "testRunner":"xunit"
